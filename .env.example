# 阿里云DashScope API配置
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# MySQL数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=medical_user
MYSQL_PASSWORD=medical_password_2024
MYSQL_DATABASE=medical_knowledge_base

# Flask应用配置
FLASK_APP=src/app.py
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# RAG配置
EMBEDDING_MODEL=text-embedding-v2
LLM_MODEL=qwen-plus-2025-07-28
MULTIMODAL_MODEL=qwen-vl-plus
KNOWLEDGE_BASE_PATH=./knowledge_base/medical

# 智能体配置
AGENT_MODEL=qwen-plus-latest
AGENT_MAX_ITERATIONS=10

# 安全配置
ENABLE_SAFETY_CHECK=True
ENABLE_ETHICS_CHECK=True
ENABLE_HUMANISTIC_CARE=True

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/medical_assistant.log
